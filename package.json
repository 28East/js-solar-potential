		"test:unit": "vitest"
	},
	"dependencies": {
		"dotenv": "^16.3.1"
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.5",
		"@googlemaps/js-api-loader": "^1.16.2",
		"@material/web": "^1.0.0-pre.14",
		"@neoconfetti/svelte": "^1.0.0",
		"@playwright/test": "^1.36.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.22.4",
		"@types/cookie": "^0.5.1",
		"@types/google.maps": "^3.53.5",
		"@types/proj4": "^2.5.2",
