		"proj4": "^2.9.0",
		"svelte": "^4.1.2",
		"svelte-check": "^3.4.6",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.7",
		"vitest": "^0.33.0"
